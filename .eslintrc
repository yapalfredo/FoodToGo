{
    "env": {
        "es2021": true
    },
    "extends": "@react-native-community",
    
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "quotes": [2, "double", { "avoidEscape": true }],
        "prettier/prettier": [
            "error",
            {
              "endOfLine": "auto"
            }
          ]
        
    }
}
